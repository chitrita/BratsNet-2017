
net: "BratsNet-V4-train.prototxt"

####### inverse learning rate
lr_policy: "inv"
base_lr: 0.01
momentum: 0.9
weight_decay: 0.0005
gamma: 0.0001
power: 0.75

# Display every 100 iterations
display: 10
iter_size: 2
# The maximum number of iterations
max_iter: 100000
# snapshot intermediate results
snapshot: 1000
snapshot_prefix: path to directroy for saving model file
# solver mode: CPU or GPU
solver_mode: GPU 


